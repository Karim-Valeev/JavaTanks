<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox fx:id="vbox" layoutX="10.0" layoutY="10.0" prefHeight="250.0" prefWidth="300.0" spacing="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="ru.kpfu.itis.Game">
    <style>
        -fx-padding: 10;
        -fx-border-style: solid inside;
        -fx-border-width: 2;
        -fx-border-insets: 5;
        -fx-border-radius: 5;
        -fx-border-color: blue;
    </style>
    <children>
        <Button fx:id="quitButton" alignment="CENTER_RIGHT" contentDisplay="CENTER" mnemonicParsing="false" text="quit" textAlignment="CENTER" onAction="#exit" />
        <Button fx:id="infoButton" alignment="CENTER_RIGHT" contentDisplay="CENTER" mnemonicParsing="false" text="guide" textAlignment="CENTER" onAction="#newScene" />
    </children>
</HBox>